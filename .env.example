# Configurações do Supabase
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configurações da Aplicação
VITE_APP_NAME="OptiStore - Gestão de Loja"
VITE_APP_VERSION="1.0.0"

# Configurações de Upload
VITE_UPLOAD_MAX_SIZE=10485760
VITE_ALLOWED_FILE_TYPES="image/jpeg,image/png,application/pdf,text/plain"

# Configurações de Notificação
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_NOTIFICATION_SOUND=true

# Configurações de Chat
VITE_CHAT_MAX_MESSAGE_LENGTH=1000
VITE_CHAT_FILE_UPLOAD_ENABLED=true

# Configurações de Desenvolvimento
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_METRICS=false